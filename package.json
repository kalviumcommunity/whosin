{
  "name": "whosin",
  "version": "1.0.0",
  "description": "open-source attendance tracking server",
  "type": "module",
  "main": "src/server.ts",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "pretty": "prettier --write \"src/**/*.ts\""
  },
  "author": "th3c0d3br34ker",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "chalk": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "nanoid": "^4.0.2",
    "redis": "^4.6.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "husky": "^8.0.0"
  }
}
